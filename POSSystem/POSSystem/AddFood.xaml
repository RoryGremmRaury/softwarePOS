<Window x:Class="POSSystem.AddFood"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:POSSystem"
        mc:Ignorable="d"
        Title="AddFood" Height="450" Width="800">
    <Grid>
        <Button x:Name="back" Content="Back" HorizontalAlignment="Left" Margin="46,246,0,0" VerticalAlignment="Top" Width="127" Click="back_Click" Height="118"/>

        <TextBox x:Name="priceTB" Text="" Height="50" Width="600" Background="White" Margin="96,144,96,225" />
        <TextBox x:Name="nameTB" Text="" Height="50" Width="600" Background="White" Margin="96,56,96,313" />
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="120,33,0,0" TextWrapping="Wrap" Text="Name" VerticalAlignment="Top"/>
        <TextBlock x:Name="textBlock_Copy" HorizontalAlignment="Left" Margin="96,111,0,0" TextWrapping="Wrap" Text="price" VerticalAlignment="Top"/>
        <Button x:Name="submitfood" Content="submit" HorizontalAlignment="Left"  Click="submitfood_Click" Margin="457,295,0,0" VerticalAlignment="Top" Width="75"/>
    </Grid>
</Window>
